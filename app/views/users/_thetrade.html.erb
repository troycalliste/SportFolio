<%= simple_form_for @user do |b| %>
  <%= b.simple_fields_for(:trades, @user.trades.build) do |p| %>
    <%= p.input :stock %>
    <%= p.input :stockprice %>
    <%= p.input :region_id %>
    <%= p.input :tradetype %>
    <%= p.input :volume %>
    <br>
    <%= p.button :submit %>
  <% end %>
<% end %>
